FILE1 = sar_interp1.c sar_kernel1_driver.c sar_utils.c timer.c

CC = arm-linux-gnueabi-gcc
CC_FLAGS = -static -Iinc -DINPUT_SIZE=INPUT_SIZE_LARGE
LDFLAGS := -lm -lrt
ISA_FILE = /home/amir/AMHM_Approximation_Benchmarks/pfa/m5op_arm.S

all: sobel

sobel: ${FILE1} Makefile
	$(CC)  $(CC_FLAGS) ${FILE1} -o pfa_interp1 $(LDFLAGS) $(ISA_FILE)

clean:
	rm -rf pfa_interp1 output*
